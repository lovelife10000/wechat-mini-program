<template name="banner">
  <swiper class="swiper" indicator-dots="true" autoplay="true" interval="5000" duration="500">
    <block wx:for="{{bannerData}}" wx:key="key">
      <swiper-item>
        <image src="{{item.url}}" class="slide-image" mode="aspectFill" />
      </swiper-item>
    </block>
  </swiper>

</template>